(
    boxToCell
    {
        box (0.05 0 0) (0.1 0.1 0.1);
        fieldValues
        (
	    volVectorFieldValue U (0.026 0 0)
            volScalarFieldValue T 2000
            volScalarFieldValue H2O 1
        );
    }
